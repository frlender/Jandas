{
  "name": "jandas",
  "version": "1.1.9",
  "description": "A very much Pandas-like JavaScript library for data science",
  "main": "lib/J.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepare": "npm run build && ./node_modules/.bin/esbuild lib/J.js --minify --keep-names --bundle --global-name=jandas --platform=browser --outfile=dist/jandas.min.js",
    "prepublishOnly": "npm test"
  },
  "author": "Qiaonan Duan",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.191",
    "@stdlib/types": "^0.4.1",
    "@jest/globals": "29.5.0",
    "esbuild": "0.17.5",
    "jest": "29.5.0",
    "ts-jest": "29.1.1",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frlender/Jandas"
  },
  "keywords": [
    "pandas",
    "DataFrame",
    "Series",
    "data analysis",
    "indexing"
  ],
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "dependencies": {
    "@stdlib/stats-ranks": "^0.2.2",
    "lodash": "^4.17.21",
    "simple-statistics": "^7.8.3"
  }
