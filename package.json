    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "@types/d3-array": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frlender/Jandas"
  },
  "keywords": [
    "pandas",
